TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_FROM_NUMBER=+10000000000
# Optional: comma-separated list of FROM numbers for rotation.
# If set, outbound calls will round-robin across this pool.
# If not set, TWILIO_FROM_NUMBER is used for all calls.
# Example:
# TWILIO_FROM_NUMBER_POOL="+18722686738,+18594313796,+14247226435"
TWILIO_FROM_NUMBER_POOL=
PUBLIC_BASE_URL=https://your-ngrok-url-or-domain
VOICEMAIL_AUDIO_URL=https://your-voicemail-audio-url.mp3
PORT=3000

# Stripe billing
STRIPE_SECRET_KEY=sk_test_xxx
STRIPE_METERED_PRICE_ID=price_xxx          # metered price id (per-call)
STRIPE_WEBHOOK_SECRET=whsec_xxx           # Stripe webhook signing secret (for /webhooks/stripe)
STRIPE_BILLING_PORTAL_RETURN_URL=http://localhost:5173/app/settings
